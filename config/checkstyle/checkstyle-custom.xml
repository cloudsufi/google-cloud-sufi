<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Puppy Crawl//DTD Check Configuration 1.3//EN"
        "http://www.puppycrawl.com/dtds/configuration_1_3.dtd">
<module name="Checker">
    <property name="charset" value="UTF-8"/>

    <property name="localeLanguage" value="en"/>

    <module name="SuppressionSingleFilter">
        <property name="checks" value="MissingDeprecated"/>
        <property name="files" value="(User|Prospect|Enrollment)[.]java"/>
    </module>

    <module name="SuppressionSingleFilter">
        <property name="checks" value="IllegalType"/>
        <property name="files" value="(Claims)[.]java"/>
    </module>

    <!--<module name="FileLength">-->
    <!--<property name="fileExtensions" value="java" />-->
    <!--<property name="max" value="1000" />-->
    <!--</module>-->

    <module name="FileTabCharacter">
        <property name="eachLine" value="true"/>
        <!--<property name="fileExtensions" value="asciidoc,adoc,css,csv,java,markdown,md,properties,sh,sql,txt,xml,xsd,xsl,yml" />-->
    </module>

    <!--<module name="NewlineAtEndOfFile">-->
    <!--<property name="fileExtensions" value="asciidoc,java,properties" />-->
    <!--<property name="lineSeparator" value="lf" />-->
    <!--</module>-->

    <module name="RegexpMultiline">
        <property name="format" value="\r\n$"/>
        <property name="message" value="Lines should end with Unix-like end of line (EOL)"/>
    </module>

    <module name="RegexpSingleline">
        <property name="format"
                  value="newArrayList\(\)|newHashSet\(\)|newLinkedList\(\)|newLinkedHashSet\(\)|newTreeSet\(\)|newHashMap\(\)|newLinkedHashMap\(\)|newTreeMap\(\)"/>
        <property name="message"
                  value="These methods are now unnecessary and should be treated as deprecated. Instead, use the appropriate no-arg constructor directly, taking advantage of the new diamond syntax: newArrayList(), newHashSet(), newTreeSet(), newHashMap(), newLinkedList(), newTreeMap(), newLinkedHashSet(), newLinkedHashMap()"/>
        <property name="minimum" value="0"/>
        <property name="maximum" value="0"/>
    </module>

    <module name="RegexpSingleline">
        <property name="format"
                  value="com.google.common.base.Objects"/>
        <property name="message"
                  value="The methods in com.google.common.base.Objects class should be treated as deprecated; use the methods from java.util.Objects instead"/>
        <property name="minimum" value="0"/>
        <property name="maximum" value="0"/>
    </module>

    <module name="RegexpSingleline">
        <property name="format"
                  value="Objects.equal\("/>
        <property name="message"
                  value="com.google.common.base.Objects#equal should be treated as deprecated; use java.util.Objects#equals instead"/>
        <property name="minimum" value="0"/>
        <property name="maximum" value="0"/>
    </module>

    <module name="RegexpSingleline">
        <property name="format"
                  value="Objects.hashCode\("/>
        <property name="message"
                  value="com.google.common.base.Objects#hashCode should be treated as deprecated; use java.util.Objects#hash instead"/>
        <property name="minimum" value="0"/>
        <property name="maximum" value="0"/>
    </module>

    <!--<module name="RegexpMultiline">-->
    <!--<property name="fileExtensions" value="java" />-->
    <!--<property name="format" value="\r?\n[ \t\x0B\f]*\r?\n[ \t\x0B\f]*\r?\n" />-->
    <!--<property name="message" value="Two consecutive empty lines are not allowed" />-->
    <!--</module>-->

    <!--<module name="RegexpSingleline">-->
    <!--<property name="format" value="\s+$" />-->
    <!--<property name="message" value="Trailing spaces are not allowed" />-->
    <!--</module>-->

    <!-- Checks that property files contain the same keys -->
    <module name="Translation">
        <property name="baseName" value="^messages.*$"/>
        <property name="fileExtensions" value="properties"/>
    </module>

    <module name="TreeWalker">
        <!--<module name="AnnotationUseStyle">-->
        <!--<property name="closingParens" value="ignore" />-->
        <!--<property name="elementStyle" value="compact_no_array" />-->
        <!--<property name="trailingArrayComma" value="never" />-->
        <!--</module>-->

        <module name="MissingDeprecated"/>

        <module name="MissingOverride"/>

        <module name="PackageAnnotation"/>

        <module name="SuppressWarnings"/>

        <!-- Checks for blocks -->
        <module name="AvoidNestedBlocks"/>

        <module name="EmptyBlock"/>

        <!--<module name="LeftCurly" />-->

        <!--<module name="NeedBraces" />-->

        <module name="RightCurly"/>

        <!-- Checks for class design -->
        <module name="DesignForExtension">
            <property name="severity" value="ignore"/>
        </module>

        <module name="InterfaceIsType"/>

        <!--<module name="InnerTypeLast" />-->

        <module name="ThrowsCount"/>

        <!--<module name="VisibilityModifier" />-->

        <!-- Checks for common coding problems -->
        <!--<module name="ArrayTrailingComma" />-->

        <module name="CovariantEquals"/>

